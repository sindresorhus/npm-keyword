{
	"name": "npm-keyword",
	"version": "7.0.0",
	"description": "Get a list of npm packages with keywords",
	"license": "MIT",
	"repository": "sindresorhus/npm-keyword",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		"types": "./index.d.ts",
		"default": "./index.js"
	},
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
